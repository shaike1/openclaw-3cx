# Voice v2 Environment Configuration
# Copy this file to .env and fill in your values

# SIP Configuration
SBC_PUBLIC_IP=1664.3cx.cloud
DRACHTIO_SECRET=cymru
FREESWITCH_SECRET=ClueCon

# OpenClaw Gateway
OPENCLAW_TOKEN=your-token-here
OPENCLAW_GATEWAY_URL=http://127.0.0.1:18789

# STT/TTS Configuration
PRIMARY_STT=google-cloud
FALLBACK_STT=openai-whisper
PRIMARY_TTS=google-cloud
FALLBACK_TTS=openai-tts

# Google Cloud TTS
GOOGLE_CLOUD_TTS_KEY_PATH=/app/keys/google-tts-key.json

# OpenAI Whisper
OPENAI_WHISPER_KEY=your-openai-key-here

# Session Configuration
SESSION_PREFIX=call-
SESSION_LOCK_RETRY_COUNT=3
SESSION_LOCK_RETRY_DELAY_MS=500

# Barge-in Configuration
BARGE_IN_ENABLED=true
VAD_THRESHOLD=0.5

# Health Check Port
HEALTH_PORT=3100

# Logging
LOG_LEVEL=info
